;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,CAAA;AACA;AACA,OALA;AAMA,KArBA;;AAsBA,IAAA,OAAA,CAAA,KAAA,EAAA,CAAA,EAAA;AACA,WAAA,QAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,iBAAA,CAAA,EAAA,EAAA,IAAA,CAAA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,CAAA;AACA;AACA,OALA;AAMA,KA9BA;;AA+BA,IAAA,KAAA,GAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,UAAA,EAAA,IAAA,CAAA,GAAA,IAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA;AACA,OAJA;AAKA,WAAA,KAAA,CAAA,GAAA,CAAA,QAAA,EAAA,IAAA,CAAA,GAAA,IAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA;AACA,OAJA;AAKA;;AA5CA,GATA;;AAuDA,EAAA,OAAA,GAAA;AACA,SAAA,OAAA;AACA,SAAA,IAAA;AACA,GA1DA;;AA2DA,EAAA,KAAA,EAAA;AACA,IAAA,QAAA,CAAA,QAAA,EAAA,QAAA,EAAA;AACA,UAAA,QAAA,IAAA,IAAA,EAAA;AACA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,KAAA;AACA;;AACA,UAAA,QAAA,IAAA,SAAA,EAAA;AACA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;;AACA,UAAA,QAAA,IAAA,QAAA,EAAA;AACA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;;AACA,WAAA,QAAA,GAAA,QAAA;AACA;;AAfA;AA3DA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"zuofloat\">\r\n      <div class=\"shujia\" v-if=\"$store.state.islogin\">\r\n        <img :src=\"$store.state.pic\" alt=\"\" class=\"logo\" />\r\n        <p class=\"login\">{{ $store.state.name }}的书架</p>\r\n      </div>\r\n      <div v-else class=\"shujia\">\r\n        <img src=\"images/img/login/avatar-default.png\" alt=\"\" class=\"logo\" />\r\n        <p class=\"login\">我的书架</p>\r\n      </div>\r\n    </div>\r\n    <div class=\"ours\">\r\n      <p class=\"yuedu\">我的关注</p>\r\n      <p class=\"guanzhu\" @click=\"clear\">一键清空</p>\r\n      <div class=\"shezhi el-icon-setting\">\r\n        <router-link to=\"/quit\" class=\"quit\">设置</router-link>\r\n      </div>\r\n    </div>\r\n    <div class=\"dibu\" v-if=\"$store.state.islogin\">\r\n      <div v-for=\"(item, index) in data\" :key=\"'item.m_id' + index\">\r\n        <div class=\"no\">\r\n          <router-link :to=\"`/article?m_id=${item.m_id}`\">\r\n            <img :src=\"item.m_img\" alt=\"\" class=\"pic\" />\r\n            <p class=\"font\">{{ item.m_title }}</p>\r\n            <p class=\"font\">{{ item.m_kind }}</p>\r\n          </router-link>\r\n\r\n          <button @click=\"dellike(index, item.m_id)\" class=\"you\">取消</button>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-for=\"(item, index) in lovelist\" :key=\"'item.i_id' + index\">\r\n        <div class=\"no\">\r\n          <router-link :to=\"`/cooperation?i_id=${item.i_id}`\">\r\n            <img :src=\"item.i_img\" alt=\"\" class=\"pic\" />\r\n            <p class=\"font\">{{ item.i_title }}</p>\r\n            <p class=\"font\">{{ item.i_kind }}</p>\r\n          </router-link>\r\n\r\n          <button @click=\"dellove(index, item.i_id)\" class=\"you\">取消</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div v-else>\r\n      <img src=\"images/img/Original/查询不到.png\" alt=\"\" class=\"nothing\" />\r\n    </div>\r\n    <!-- 底部选项卡 -->\r\n    <mt-tabbar v-model=\"selected\" fixed>\r\n      <mt-tab-item id=\"tuijian\"\r\n        >推荐\r\n        <img\r\n          slot=\"icon\"\r\n          :src=\"\r\n            require(`../assets/common/cart_${\r\n              selected == 'tuijian' ? '1' : '0'\r\n            }.png`)\r\n          \"\r\n          alt=\"\"\r\n        />\r\n      </mt-tab-item>\r\n      <mt-tab-item id=\"faxian\"\r\n        >发现\r\n        <img\r\n          slot=\"icon\"\r\n          :src=\"\r\n            require(`../assets/common/main_${\r\n              selected == 'faxian' ? '1' : '0'\r\n            }.png`)\r\n          \"\r\n          alt=\"\"\r\n        />\r\n      </mt-tab-item>\r\n      <mt-tab-item id=\"shujia\"\r\n        >书架\r\n        <img\r\n          slot=\"icon\"\r\n          :src=\"\r\n            require(`../assets/common/book_${\r\n              selected == 'shujia' ? '1' : '0'\r\n            }.png`)\r\n          \"\r\n          alt=\"\"\r\n        />\r\n      </mt-tab-item>\r\n      <mt-tab-item id=\"me\"\r\n        >我的\r\n        <img\r\n          slot=\"icon\"\r\n          :src=\"\r\n            require(`../assets/common/me_${selected == 'me' ? '1' : '0'}.png`)\r\n          \"\r\n          alt=\"\"\r\n        />\r\n      </mt-tab-item>\r\n    </mt-tabbar>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      selected: \"shujia\", //指定底部选项卡默认选中项id\r\n      data: \"\",\r\n      lovelist: \"\",\r\n      token: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    getData() {\r\n      this.axios.get(\"/selectlike\").then((res) => {\r\n        console.log(res);\r\n        this.data = res.data.data;\r\n      });\r\n    },\r\n    init() {\r\n      this.axios.get(\"/selectlove\").then((res) => {\r\n        console.log(res);\r\n        this.lovelist = res.data.data;\r\n      });\r\n    },\r\n    dellike(index, e) {\r\n      this.data.splice(index, 1);\r\n      this.axios.get(`/del?m_id=${e}`).then((res) => {\r\n        console.log(res);\r\n        if (res.data.code == 1) {\r\n          console.log(\"删除成功\",e);\r\n        }\r\n      });\r\n    },\r\n    dellove(index, e) {\r\n      this.lovelist.splice(index, 1);\r\n      this.axios.get(`/lovedel?i_id=${e}`).then((res) => {\r\n        console.log(res);\r\n        if (res.data.code == 1) {\r\n          console.log(\"删除成功\",e);\r\n        }\r\n      });\r\n    },\r\n    clear() {\r\n      this.lovelist = [];\r\n      this.data = [];\r\n      this.axios.get(\"/likedel\").then((res) => {\r\n        if (res.data.code == 1) {\r\n          console.log(\"一键清空成功\");\r\n        }\r\n      });\r\n      this.axios.get(\"/ipdel\").then((res) => {\r\n        if (res.data.code == 1) {\r\n          console.log(\"一键清空成功\");\r\n        }\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n    this.init();\r\n  },\r\n  watch: {\r\n    selected(newValue, oldValue) {\r\n      if (newValue == \"me\") {\r\n        //用户点了底部选项:我的\r\n        this.$router.push(\"/me\");\r\n      }\r\n      if (newValue == \"tuijian\") {\r\n        //用户点了底部选项:推荐\r\n        this.$router.push(\"/recom\");\r\n      }\r\n      if (newValue == \"faxian\") {\r\n        //用户点了底部选项:发现\r\n        this.$router.push(\"/\");\r\n      }\r\n      this.selected = \"shujia\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped src=\"../assets/css/book.css\">\r\n</style>"],"sourceRoot":"src/views"}]}