I,EAAE,QADR;AAEEE,EAAAA,SAAS,EAAEX;AAFb,CA1Ba,EA8Bb;AACES,EAAAA,IAAI,EAAE,WADR;AAEEE,EAAAA,SAAS,EAAEZ;AAFb,CA9Ba,EAkCb;AACEU,EAAAA,IAAI,EAAE,QADR;AAEEE,EAAAA,SAAS,EAACb;AAFZ,CAlCa,EAsCb;AACEW,EAAAA,IAAI,EAAE,SADR;AAEEE,EAAAA,SAAS,EAACd;AAFZ,CAtCa,EA0Cb;AACEY,EAAAA,IAAI,EAAE,SADR;AAEEE,EAAAA,SAAS,EAACf;AAFZ,CA1Ca,EA8Cb;AACEa,EAAAA,IAAI,EAAE,GADR;AAEEC,EAAAA,IAAI,EAAE,MAFR;AAGEC,EAAAA,SAAS,EAAEhB,IAHb;AAIEiB,EAAAA,IAAI,EAAC;AAACC,IAAAA,SAAS,EAAC;AAAX;AAJP,CA9Ca,EAoDb;AACEJ,EAAAA,IAAI,EAAE,QADR;AAEEC,EAAAA,IAAI,EAAE,OAFR;AAGE;AACA;AACA;AACAC,EAAAA,SAAS,EAAE;AAAA,WAAM;AAAO;AAAgC,wBAAvC,CAAN;AAAA;AANb,CApDa,CAAf;AA8DA,IAAMG,MAAM,GAAG,IAAIpB,SAAJ,CAAc;AAC3BqB,EAAAA,IAAI,EAAE,SADqB;AAE3BC,EAAAA,IAAI,EAAEC,OAAO,CAACC,GAAR,CAAYC,QAFS;AAG3BX,EAAAA,MAAM,EAANA,MAH2B;AAI3B;AACAY,EAAAA,cAL2B,0BAKXC,EALW,EAKPC,IALO,EAKDC,aALC,EAKc;AACvC,QAAGD,IAAI,CAACZ,IAAL,IAAW,MAAX,IAAqBW,EAAE,CAACX,IAAH,IAAS,MAAjC,EAAwC;AACtC;AACA,aAAO;AAACc,QAAAA,CAAC,EAAC,CAAH;AAAKC,QAAAA,CAAC,EAAC;AAAP,OAAP;AACD,KAHD,MAGK;AACH;AACA,aAAOF,aAAP;AACD;AAEF;AAd0B,CAAd,CAAf;AAiBA,eAAeT,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport Home from '../views/Home.vue'\nimport Button from '../testing/Button.vue'\nimport Header from '../testing/Header.vue'\nimport Field from '../testing/Field.vue'\nimport Register from '../views/Register.vue'\nimport Login from '../views/Login.vue'\nimport Navbar from '../testing/Navbar.vue'\nimport Tabbar from '../testing/Tabbar.vue'\nimport Me from '../views/Me.vue'\nimport Swipe from '../testing/Swipe.vue'\nimport Inf from '../testing/Inf.vue'\nimport Article from '../views/Article.vue'\n\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/article',\n    name:'article',\n    component: Article,\n  },\n  {\n    path: '/inf',\n   component: Inf,\n  },\n  {\n    path: '/swipe',\n    component: Swipe,\n  },\n  {\n    path: '/me',\n    component: Me,\n  },\n  {\n    path: '/tabbar',\n    component:Tabbar,\n  },\n  {\n    path: '/navbar',\n    component: Navbar,\n  },\n  {\n    path: '/login',\n    component: Login,\n  },\n  {\n    path: '/register',\n    component: Register,\n  },\n  {\n    path: '/field',\n    component:Field,\n  },\n  {\n    path: '/header',\n    component:Header,\n  },\n  {\n    path: '/button',\n    component:Button,\n  },\n  {\n    path: '/',\n    name: 'Home',\n    component: Home,\n    meta:{keepAlive:true}\n  },\n  {\n    path: '/about',\n    name: 'About',\n    // route level code-splitting\n    // this generates a separate chunk (about.[hash].js) for this route\n    // which is lazy-loaded when the route is visited.\n    component: () => import(/* webpackChunkName: \"about\" */ '../views/About.vue')\n  }\n]\n\nconst router = new VueRouter({\n  mode: 'history',\n  base: process.env.BASE_URL,\n  routes,\n  //刷新回到首页导航栏,无论在哪个导航栏\n  scrollBehavior (to, from, savedPosition) {\n    if(from.name=='Home' && to.name=='Home'){\n      //刷新首页,回到顶部\n      return {x:0,y:0}\n    }else{\n      //从详情页返回回来\n      return savedPosition\n    }\n    \n  }\n})\n\nexport default router\n"]}]}