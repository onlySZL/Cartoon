,EAAAA,IAAI,EAAE,WADR;AAEEE,EAAAA,SAAS,EAAEjB;AAFb,CA9Fa,EAkGb;AACEe,EAAAA,IAAI,EAAE,GADR;AAEEC,EAAAA,IAAI,EAAE,MAFR;AAGEC,EAAAA,SAAS,EAAElB,IAHb;AAIE8B,EAAAA,IAAI,EAAC;AAACC,IAAAA,SAAS,EAAC;AAAX;AAJP,CAlGa,EAwGb;AACEf,EAAAA,IAAI,EAAE,QADR;AAEEC,EAAAA,IAAI,EAAE,OAFR;AAGE;AACA;AACA;AACAC,EAAAA,SAAS,EAAE,MAAM;AAAO;AAAgC,sBAAvC;AANnB,CAxGa,CAAf;AAmHA,MAAMc,MAAM,GAAG,IAAIjC,SAAJ,CAAc;AAC3BkC,EAAAA,IAAI,EAAE,SADqB;AAE3BC,EAAAA,IAAI,EAAEC,OAAO,CAACC,GAAR,CAAYC,QAFS;AAG3BtB,EAAAA,MAH2B;;AAI3B;AACAuB,EAAAA,cAAc,CAAElB,EAAF,EAAMC,IAAN,EAAYkB,aAAZ,EAA2B;AACvC,QAAGlB,IAAI,CAACJ,IAAL,IAAW,MAAX,IAAqBG,EAAE,CAACH,IAAH,IAAS,OAAjC,EAAyC;AACvC;AACA,aAAO;AAACuB,QAAAA,CAAC,EAAC,CAAH;AAAKC,QAAAA,CAAC,EAAC;AAAP,OAAP;AACD,KAHD,MAGM,IAAGpB,IAAI,CAACJ,IAAL,IAAW,OAAX,IAAsBG,EAAE,CAACH,IAAH,IAAS,MAAlC,EAAyC;AAC5C;AACA,aAAO;AAACuB,QAAAA,CAAC,EAAC,CAAH;AAAKC,QAAAA,CAAC,EAAC;AAAP,OAAP;AACF,KAHK,MAGD;AACH,aAAOF,aAAP;AACD;AAEF;;AAhB0B,CAAd,CAAf;AAqBA,eAAeP,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport Home from '../views/Home.vue'\nimport Register from '../views/Register.vue'\nimport Me from '../views/Me.vue'\nimport Login from '../views/Login.vue'\nimport Book from '../views/Book.vue'\nimport Recom from '../views/Recom.vue'\nimport Classify from '../views/Classify.vue'\n\nconst originalPush = VueRouter.prototype.push\n\nVueRouter.prototype.push = function push(location) {\n  return originalPush.call(this, location).catch(err => err)\n}\n\nVue.use(VueRouter)\n\n\nconst routes = [\n  {\n    path: '/selectes',\n    name: 'selectes',\n    component: () => import('../views/Selectes.vue'),\n  },\n  {\n    path: '/select',\n    name: 'select',\n    component: () => import('../views/Select.vue'),\n  },\n  {\n    path: '/comment',\n    name: 'comment',\n    component: () => import('../views/Comment.vue'),\n  },\n  {\n    path: '/quit',\n    name: 'quit',\n    component: () => import('../views/Quit.vue'),\n  },\n  {\n    path: '/myself',\n    name: 'myself',\n    component: () => import('../views/Myself.vue'),\n    beforeEnter: (to, from, next) => {\n      console.log(to)\n      window.sessionStorage.setItem(\"toPath\",to.fullPath)\n      if(window.sessionStorage.getItem(\"islogin\")){\n        next()\n      }else{\n        next('/login')\n      }\n    }\n  },\n  {\n    path: '/cooperation',\n    name: 'cooperation',\n    component: () => import('../views/Cooperation.vue'),\n    beforeEnter: (to, from, next) => {\n      console.log(to)\n      window.sessionStorage.setItem(\"toPath\",to.fullPath)\n      if(window.sessionStorage.getItem(\"islogin\")){\n        next()\n      }else{\n        next('/login')\n      }\n    }\n  },\n  {\n    path: '/article',\n    name: 'article',\n    component: () => import('../views/Article.vue'),\n    beforeEnter: (to, from, next) => {\n      console.log(to)\n      window.sessionStorage.setItem(\"toPath\",to.fullPath)\n      if(window.sessionStorage.getItem(\"islogin\")){\n        next()\n      }else{\n        next('/login')\n      }\n    }\n  },\n  {\n    path: '/personal',\n    name: 'personal',\n    component: () => import('../views/Personal.vue'),\n  \n  },\n\n  {\n    path: '/classify',\n    component: Classify,\n  },\n  {\n    path: '/recom',\n    name:'recom',\n    component: Recom,\n    meta:{keepAlive:true}\n  },\n  {\n    path: '/book',\n    component: Book,\n  },\n  {\n    path: '/login',\n    component: Login,\n    name:'Login'\n  },\n{\n    path: '/me',\n    component: Me,\n    name:'Me'\n  },      \n  {\n    path: '/register',\n    component: Register,\n  },\n  {\n    path: '/',\n    name: 'Home',\n    component: Home,\n    meta:{keepAlive:true}\n  },\n  {\n    path: '/about',\n    name: 'About',\n    // route level code-splitting\n    // this generates a separate chunk (about.[hash].js) for this route\n    // which is lazy-loaded when the route is visited.\n    component: () => import(/* webpackChunkName: \"about\" */ '../views/About.vue')\n  }\n]\n\n\nconst router = new VueRouter({\n  mode: 'history',\n  base: process.env.BASE_URL,\n  routes,\n  //刷新回到首页导航栏,无论在哪个导航栏\n  scrollBehavior (to, from, savedPosition) {\n    if(from.name=='Home' && to.name=='recom'){\n      //刷新首页,回到顶部\n      return {x:0,y:0}\n    }else if(from.name=='recom' && to.name=='Home'){\n       //刷新首页,回到顶部\n       return {x:0,y:0}\n    }else{\n      return savedPosition;\n    }\n    \n  }\n \n})\n\n\nexport default router\n"]}]}