 this.m_kind = \"\";\r\n      }\r\n      this.axios.post(\"/page\", `m_kind=${this.m_kind}`).then((res) => {\r\n        console.log(res);\r\n        this.classlist = res.data.data;\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.axios.post(\"/page\", `m_kind=${this.m_kind}`).then((res) => {\r\n      console.log(res);\r\n      this.classlist = res.data.data;\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped src=\"../assets/css/classify.css\">\r\n</style>"],"sourceRoot":"src/views"}]}