{"remainingRequest":"C:\\web2019\\2-XM\\demo\\scaffolding\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\web2019\\2-XM\\demo\\scaffolding\\src\\views\\Login.vue?vue&type=style&index=1&sc=true&lang=css&","dependencies":[{"path":"C:\\web2019\\2-XM\\demo\\scaffolding\\src\\views\\Login.vue","mtime":1642662829620},{"path":"C:\\web2019\\2-XM\\demo\\scaffolding\\node_modules\\css-loader\\dist\\cjs.js","mtime":1640758916568},{"path":"C:\\web2019\\2-XM\\demo\\scaffolding\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1640758930182},{"path":"C:\\web2019\\2-XM\\demo\\scaffolding\\node_modules\\postcss-loader\\src\\index.js","mtime":1640758922007},{"path":"C:\\web2019\\2-XM\\demo\\scaffolding\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1640758908837},{"path":"C:\\web2019\\2-XM\\demo\\scaffolding\\node_modules\\vue-loader\\lib\\index.js","mtime":1640758924784}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Login.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Login.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <img src=\"/images/img/Home/logo2.png\" alt=\"\" class=\"logo\" />\r\n    </div>\r\n    <p class=\"login\">登录</p>\r\n\r\n    <div class=\"reg\">\r\n    \r\n     <!-- 输入框 -->\r\n    <mt-field\r\n     class=\"fit\"\r\n      type=\"text\"\r\n      label=\"用户名\"\r\n      @blur.native.capture=\"checkName\"\r\n      v-model=\"name\"\r\n      :state=\"nameState\"\r\n      placeholder=\"请输入用户名\"\r\n    ></mt-field>\r\n    <mt-field\r\n      class=\"fit\"\r\n      type=\"password\"\r\n      label=\"密码\"\r\n      @blur.native.capture=\"checkPwd\"\r\n      placeholder=\"请输入密码\"\r\n      :attr=\"{ maxlength: '11' }\"\r\n      v-model=\"pwdname\"\r\n      :state=\"pwdState\"\r\n    ></mt-field>\r\n    <mt-button class=\"btn\" @click=\"checkForm\"  size=\"large\"\r\n      >登录</mt-button\r\n    >\r\n\r\n    </div>\r\n\r\n    <div>\r\n        <p class=\"way\">其它登录方式</p>\r\n        <router-link to=\"/register\" class=\"go\">去注册</router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n   data() {\r\n    return {\r\n      name: \"\", //绑定用户名文本框输入的内容\r\n      nameState: \"\", //绑定用户名文本框的状态\r\n      pwdname: \"\", //绑定密码文本框输入的内容\r\n      pwdState: \"\", //绑定密码文本框输入的状态\r\n    };\r\n  },\r\n  methods: {\r\n    //验证用户名文本框\r\n    checkName() {\r\n      let reg = /^[a-zA-Z0-9_-]{4,16}$/;\r\n      if (reg.test(this.name)) {\r\n        //验证成功\r\n        this.nameState = \"success\";\r\n        return true\r\n      } else {\r\n        //验证失败\r\n        this.nameState = \"error\";\r\n        return false\r\n      }\r\n    },\r\n    checkPwd() {\r\n      let pwd = /^1[3-9]\\d{9}$/;\r\n      if (pwd.test(this.pwdname)){\r\n        //验证成功\r\n        this.pwdState = \"success\";\r\n        return true\r\n      } else {\r\n        //验证失败\r\n        this.pwdState = \"error\";\r\n        return false\r\n      }                                                                                                                                                              \r\n    },\r\n    //验证表单\r\n    checkForm() {\r\n      if(this.checkName() && this.checkPwd()){\r\n      console.log(\"验证成功,执行登录业务...\");\r\n       //发送post请求,执行登录业务\r\n      this.axios.post('/login',`u_names=${this.name}&u_pwd=${this.pwdname}`).then(res=>{\r\n        console.log(res);\r\n        if(res.data.code==1){ //登录成功\r\n        this.$toast('登录成功')\r\n        this.$router.push('/')\r\n        }else if(res.data.code==0){ //登录失败\r\n         this.$messagebox('提示','账号密码输入错误')\r\n        } \r\n      })\r\n          \r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped src=\"../assets/css/login.css\">\r\n</style>\r\n\r\n<style sc>\r\n.mint-msgbox {\r\n    position: fixed;\r\n    top: 40% !important;\r\n    left: 40% !important;\r\n    transform: translate3d(-50%, -50%, 0);\r\n    background-color: #fff;\r\n    width: 85vw !important;\r\n    border-radius: 3px;\r\n    font-size: 4vw !important;\r\n    -webkit-user-select: none;\r\n    overflow: hidden;\r\n    -webkit-backface-visibility: hidden;\r\n    backface-visibility: hidden;\r\n    transition: .2s;\r\n}\r\n\r\n</style>"]}]}