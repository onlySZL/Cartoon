<mt-tab-item id=\"3\">网页</mt-tab-item>\n      <mt-tab-item id=\"4\">交互</mt-tab-item>\n    </mt-navbar>\n    <!-- 动态面板 -->\n    <mt-tab-container\n      v-model=\"active\"\n      style=\"text-align: center; margin-top: 5px\"\n    >\n      <mt-tab-container-item id=\"1\">\n        <!-- 轮播图 -->\n        <mt-swipe\n          :style=\"{ height: h }\"\n          :speed=\"3000\"\n          :atuo=\"1000\"\n          :continuous=\"false\"\n        >\n          <mt-swipe-item>\n            <img src=\"/images/1.jpg\" alt=\"\" />\n          </mt-swipe-item>\n          <mt-swipe-item>\n            <img src=\"/images/2.jpg\" alt=\"\" />\n          </mt-swipe-item>\n          <mt-swipe-item>\n            <img src=\"/images/3.jpg\" alt=\"\" />\n          </mt-swipe-item> </mt-swipe\n      ></mt-tab-container-item>\n      <mt-tab-container-item id=\"2\">电商面板</mt-tab-container-item>\n      <mt-tab-container-item id=\"3\">网页面板</mt-tab-container-item>\n      <mt-tab-container-item id=\"4\">交互面板</mt-tab-container-item>\n    </mt-tab-container>\n    <!-- 底部选项卡 -->\n    <mt-tabbar v-model=\"selected\">\n      <mt-tab-item id=\"shouye\"\n        >首页\n        <img\n          slot=\"icon\"\n          :src=\"\n            require(`../assets/common/main_${\n              selected == 'shouye' ? '1' : '0'\n            }.png`)\n          \"\n          alt=\"\"\n        />\n      </mt-tab-item>\n      <mt-tab-item id=\"me\"\n        >我的\n        <img\n          slot=\"icon\"\n          :src=\"\n            require(`../assets/common/me_${selected == 'me' ? '1' : '0'}.png`)\n          \"\n          alt=\"\"\n        />\n      </mt-tab-item>\n    </mt-tabbar>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      active: \"1\", //制顶部导航默认选项id\n      selected: \"shouye\", //指定底部选项卡默认选中项id\n      h: '210px', //轮播图的默认高度\n    };\n  },\n  watch: {\n    selected(newValue, oldValue) {\n      if (newValue == \"me\") {\n        //用户点了底部选项:我的\n        this.$router.push(\"/me\");\n      }\n    },\n  },\n  mounted(){\n            //计算轮播图的高度 为this.h变量赋值\n            let screenW = window.screen.width\n            let picw = 1242\n            let pich = 698\n            let swipeh = Math.floor(pich * screenW / picw) + 'px'\n            this.h = swipeh\n        }\n};\n</script>\n\n<style scoped>\n.link {\n  color: white;\n  margin-left: 10px;\n}\n.mint-swipe-item img{\n    width: 100% ;\n}\n</style>"]}]}